	// Basic enum with values	enum Color	{ <C_BINDER_MOJO_NEWLINE>		RED  =  0 ,	GREEN  =  1 ,	BLUE  =  2 <C_BINDER_MOJO_NEWLINE>	<C_BINDER_MOJO_NEWLINE>	};	// Enum with auto-incrementing values	enum Direction	{ <C_BINDER_MOJO_NEWLINE>		NORTH ,	// 0	SOUTH ,	// 1	EAST ,	// 2	WEST   	// 3<C_BINDER_MOJO_NEWLINE>	};	// Enum with mixed explicit and implicit values	enum Status	{ <C_BINDER_MOJO_NEWLINE>		OK  =  200 ,	NOT_FOUND  =  404 ,	SERVER_ERROR  =  500 ,	UNAVAILABLE ,	// 501	TIMEOUT     	// 502<C_BINDER_MOJO_NEWLINE>	};	// Enum with name but no values  ( empty enum ) 	enum Empty	{ <C_BINDER_MOJO_NEWLINE>	<C_BINDER_MOJO_NEWLINE>	};	// Enum with hex values and comments	enum Flags	{ <C_BINDER_MOJO_NEWLINE>		READ     =  0x01 ,	// Read permission	WRITE    =  0x02 ,	// Write permission	EXECUTE  =  0x04  	// Execute permission<C_BINDER_MOJO_NEWLINE>	};	// Enum with byte values	enum ByteValues	{ <C_BINDER_MOJO_NEWLINE>		BYTE_0  =  1 << 0 ,	BYTE_1  =  1 << 1 ,	BYTE_2  =  1 << 2 ,	BYTE_3  =  1 << 3 <C_BINDER_MOJO_NEWLINE>	<C_BINDER_MOJO_NEWLINE>	};